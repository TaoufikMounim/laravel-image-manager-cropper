{
    "name": "tasmnaguib/imagemanager",
    "description": "A Laravel Package for The managing storage file system/images",
    "keywords": [
        "laravel",
        "image manager",
        "file system manager",
        "storage manager",
        "media manager"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "ASM Mahmudul Hasan",
            "email": "asmmahmud@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/asmmahmud/laravel-image-manager-cropper/issues",
        "source": "https://github.com/asmmahmud/laravel-image-manager-cropper"
    },
    "require": {
        "illuminate/support": "~5.4.0|~5.5.0",
        "intervention/image": "~2.3"
    },
    "require-dev": {
